{
  "name": "仓库",
  "action": {
    "bind": {
      "model": "warehouse",
      "option": {
        "form": "warehouse",
        "withs": {}
      }
    }
  },
  "layout": {
    "filter": {
      "actions": [
        {
          "action": [
            {
              "type": "Common.historyPush",
              "payload": {
                "pathname": "/x/Form/warehouse/0/edit"
              },
              "name": "HistoryPush"
            }
          ],
          "title": "创建",
          "width": 3,
          "icon": "icon-plus"
        }
      ],
      "columns": [
        {
          "name": "节点"
        },
        {
          "name": "名称"
        }
      ]
    },
    "table": {
      "props": {
        "scroll": {
          "x": "max-content"
        }
      },
      "columns": [
        {
          "name": "ID",
          "width": 100
        },
        {
          "width": 160,
          "name": "节点"
        },
        {
          "name": "名称",
          "width": 160
        },
        {
          "name": "位置",
          "width": 160
        },
        {
          "name": "项目部",
          "width": 160
        },
        {
          "name": "IP地址",
          "width": 160
        },
        {
          "name": "端口",
          "width": 160
        },
        {
          "name": "密钥",
          "width": 160
        },
        {
          "name": "活跃时间",
          "width": 160
        },
        {
          "name": "公章",
          "width": 160
        },
        {
          "name": "状态",
          "width": 160
        },
        {
          "width": 160,
          "name": "创建时间"
        },
        {
          "name": "更新时间",
          "width": 160
        }
      ],
      "operation": {
        "actions": [
          {
            "title": "查看",
            "icon": "icon-eye",
            "action": [
              {
                "type": "Common.openModal",
                "payload": {
                  "Form": {
                    "type": "view",
                    "model": "warehouse"
                  }
                },
                "name": "OpenModal"
              }
            ]
          },
          {
            "title": "编辑",
            "icon": "icon-edit-2",
            "action": [
              {
                "name": "OpenModal",
                "type": "Common.openModal",
                "payload": {
                  "Form": {
                    "type": "edit",
                    "model": "warehouse"
                  }
                }
              }
            ]
          },
          {
            "title": "Delete",
            "icon": "icon-trash-2",
            "style": "danger",
            "confirm": {
              "title": "确认",
              "desc": "请确认删除，删除后数据无法恢复"
            },
            "action": [
              {
                "type": "Table.delete",
                "payload": {
                  "model": "warehouse"
                },
                "name": "Delete"
              }
            ]
          }
        ],
        "width": 140
      }
    },
    "primary": "id",
    "header": {
      "preset": {},
      "actions": []
    }
  },
  "fields": {
    "filter": {
      "名称": {
        "bind": "where.name.match",
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 名称"
          }
        }
      },
      "节点": {
        "bind": "where.node_id.match",
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 节点"
          }
        }
      }
    },
    "table": {
      "名称": {
        "bind": "name",
        "view": {
          "type": "Text",
          "props": {}
        },
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 名称"
          }
        }
      },
      "状态": {
        "bind": "status",
        "view": {
          "type": "Tag",
          "props": {
            "placeholder": "请选择 状态",
            "options": [
              {
                "label": "离线",
                "value": "离线"
              },
              {
                "label": "在线",
                "value": "在线"
              }
            ]
          }
        },
        "edit": {
          "type": "Select",
          "props": {
            "placeholder": "请选择 状态",
            "options": [
              {
                "label": "离线",
                "value": "离线"
              },
              {
                "value": "在线",
                "label": "在线"
              }
            ]
          }
        }
      },
      "更新时间": {
        "bind": "updated_at",
        "view": {
          "type": "Text",
          "props": {
            "format": "YYYY-MM-DD HH:mm:ss"
          }
        },
        "edit": {
          "type": "DatePicker",
          "props": {
            "placeholder": "请选择 更新时间",
            "showTime": {
              "format": "HH:mm:ss"
            }
          }
        }
      },
      "IP地址": {
        "bind": "ip",
        "view": {
          "type": "Text",
          "props": {}
        },
        "edit": {
          "props": {
            "placeholder": "请输入 IP地址"
          },
          "type": "Input"
        }
      },
      "密钥": {
        "bind": "secret",
        "view": {
          "type": "Text",
          "props": {}
        },
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 密钥"
          }
        }
      },
      "节点": {
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 节点"
          }
        },
        "bind": "node_id",
        "view": {
          "type": "Text",
          "props": {}
        }
      },
      "项目部": {
        "bind": "dept",
        "view": {
          "props": {},
          "type": "Text"
        },
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 项目部"
          }
        }
      },
      "公章": {
        "bind": "photo",
        "view": {
          "type": "Tooltip",
          "props": {
            "title": "公章"
          }
        },
        "edit": {
          "type": "TextArea",
          "props": {
            "placeholder": "请输入 公章",
            "autoSize": {
              "minRows": 2,
              "maxRows": 6
            }
          }
        }
      },
      "位置": {
        "bind": "location",
        "view": {
          "type": "Text",
          "props": {}
        },
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 位置"
          }
        }
      },
      "端口": {
        "bind": "port",
        "view": {
          "props": {},
          "type": "Text"
        },
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 端口"
          }
        }
      },
      "活跃时间": {
        "bind": "uptime",
        "view": {
          "type": "Text",
          "props": {
            "format": "YYYY-MM-DD HH:mm:ss"
          }
        },
        "edit": {
          "type": "DatePicker",
          "props": {
            "placeholder": "请选择 活跃时间",
            "showTime": {
              "format": "HH:mm:ss"
            }
          }
        }
      },
      "创建时间": {
        "edit": {
          "type": "DatePicker",
          "props": {
            "showTime": {
              "format": "HH:mm:ss"
            },
            "placeholder": "请选择 创建时间"
          }
        },
        "bind": "created_at",
        "view": {
          "props": {
            "format": "YYYY-MM-DD HH:mm:ss"
          },
          "type": "Text"
        }
      },
      "ID": {
        "bind": "id",
        "view": {
          "type": "Text",
          "props": {}
        },
        "edit": {
          "type": "InputNumber",
          "props": {
            "placeholder": "请输入 ID",
            "disabled": true
          }
        }
      }
    }
  }
}
