{
  "name": "导出任务下载",
  "action": {
    "bind": {
      "model": "exports",
      "form": "exports"
    },
    "search": {
      "default": [null, null, 10]
    }
  },
  "layout": {
    "header": {
      "preset": {},
      "actions": []
    },
    "table": {
      "operation": {
        "hide": true,
        "actions": []
      },
      "props": {
        "scroll": {
          "x": "max-content"
        }
      },
      "columns": [
        {
          "name": "ID",
          "width": 100
        },
        {
          "width": 160,
          "name": "名称"
        },
        {
          "name": "表格名称",
          "width": 160
        },
        {
          "name": "下载地址",
          "width": 160
        },
        {
          "name": "状态",
          "width": 160
        },
        {
          "name": "创建时间",
          "width": 160
        },
        {
          "name": "更新时间",
          "width": 160
        }
      ]
    },
    "primary": "id",
    "filter": {
      "actions": [
        {
          "action": [
            {
              "type": "Common.historyPush",
              "payload": {
                "pathname": "/x/Form/exports/0/edit"
              },
              "name": "HistoryPush"
            }
          ],
          "title": "创建",
          "width": 3,
          "icon": "icon-plus"
        }
      ]
    }
  },
  "fields": {
    "filter": {},
    "table": {
      "下载地址": {
        "view": {
          "type": "A",
          "props": {
            "href": "/api/utils/download?id={{id}}"
          }
        },
        "bind": "url"
      },
      "状态": {
        "bind": "status",
        "view": {
          "type": "Tag",
          "props": {
            "options": [
              {
                "label": "",
                "value": ""
              },
              {
                "label": "未完成",
                "value": "未完成"
              },
              {
                "label": "已完成",
                "value": "已完成"
              },
              {
                "label": "导出错误",
                "value": "导出错误"
              }
            ],
            "placeholder": "请选择 状态"
          }
        },
        "edit": {
          "type": "Select",
          "props": {
            "options": [
              {
                "value": "",
                "label": ""
              },
              {
                "label": "未完成",
                "value": "未完成"
              },
              {
                "label": "已完成",
                "value": "已完成"
              },
              {
                "label": "导出错误",
                "value": "导出错误"
              }
            ],
            "placeholder": "请选择 状态"
          }
        }
      },
      "创建时间": {
        "bind": "created_at",
        "view": {
          "type": "Text",
          "props": {
            "format": "YYYY-MM-DD HH:mm:ss"
          }
        },
        "edit": {
          "props": {
            "placeholder": "请选择 创建时间",
            "showTime": {
              "format": "HH:mm:ss"
            }
          },
          "type": "DatePicker"
        }
      },
      "更新时间": {
        "view": {
          "type": "Text",
          "props": {
            "format": "YYYY-MM-DD HH:mm:ss"
          }
        },
        "edit": {
          "type": "DatePicker",
          "props": {
            "placeholder": "请选择 更新时间",
            "showTime": {
              "format": "HH:mm:ss"
            }
          }
        },
        "bind": "updated_at"
      },
      "ID": {
        "bind": "id",
        "view": {
          "type": "Text",
          "props": {}
        },
        "edit": {
          "type": "InputNumber",
          "props": {
            "placeholder": "请输入 ID",
            "disabled": true
          }
        }
      },
      "名称": {
        "view": {
          "type": "Text",
          "props": {}
        },
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 名称"
          }
        },
        "bind": "name"
      },
      "表格名称": {
        "bind": "table_name",
        "view": {
          "type": "Text",
          "props": {}
        },
        "edit": {
          "props": {
            "placeholder": "请输入 表格名称"
          },
          "type": "Input"
        }
      }
    }
  },
  "config": {
    "full": true
  }
}
