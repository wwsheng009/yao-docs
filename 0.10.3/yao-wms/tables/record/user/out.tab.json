      },
      "创建时间": {
        "bind": "created_at",
        "view": {
          "type": "Text",
          "props": {
            "format": "YYYY-MM-DD HH:mm:ss"
          }
        },
        "edit": {
          "type": "DatePicker",
          "props": {
            "placeholder": "请选择 创建时间",
            "showTime": {
              "format": "HH:mm:ss"
            }
          }
