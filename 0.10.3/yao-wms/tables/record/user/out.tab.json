        "bind": "id"
      },
      "更新时间": {
        "edit": {
          "props": {
            "placeholder": "请选择 更新时间",
            "showTime": {
              "format": "HH:mm:ss"
            }
          },
          "type": "DatePicker"
        },
        "bind": "updated_at",
        "view": {
          "type": "Text",
          "props": {
            "format": "YYYY-MM-DD HH:mm:ss"
          }
        }
      },
      "状态": {
        "edit": {
