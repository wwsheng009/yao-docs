        }
      },
      "类型": {
        "bind": "type",
        "view": {
          "type": "Tag",
          "props": {
            "options": [
              {
                "label": "入库",
                "value": "入库"
              },
              {
                "label": "出库",
                "value": "出库"
              },
              {
                "label": "进入",
