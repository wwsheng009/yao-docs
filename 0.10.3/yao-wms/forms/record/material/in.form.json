{
  "name": "入库记录",
  "action": {
    "bind": {
      "model": "record",
      "option": {
        "withs": {}
      }
    }
  },
  "layout": {
    "primary": "id",
    "actions": [
      {
        "title": "返回",
        "icon": "icon-arrow-left",
        "showWhenAdd": true,
        "showWhenView": true,
        "action": [
          {
            "name": "CloseModal",
            "type": "Common.closeModal",
            "payload": {}
          }
        ]
      },
      {
        "title": "保存",
        "icon": "icon-check",
        "style": "primary",
        "showWhenAdd": true,
        "action": [
          {
            "name": "Submit",
            "type": "Form.submit",
            "payload": {}
          },
          {
            "name": "Back",
            "type": "Common.closeModal",
            "payload": {}
          }
        ]
      },
      {
        "icon": "icon-trash-2",
        "style": "danger",
        "title": "Delete",
        "action": [
          {
            "name": "Confirm",
            "type": "Common.confirm",
            "payload": {
              "title": "确认删除",
              "content": "删除后不可撤销！"
            }
          },
          {
            "name": "Delete",
            "payload": {
              "model": "record.material.in"
            },
            "type": "Form.delete"
          },
          {
            "name": "Back",
            "type": "Common.closeModal",
            "payload": {}
          }
        ]
      }
    ],
    "form": {
      "sections": [
        {
          "columns": [
            {
              "name": "ID",
              "width": 12
            },
            {
              "name": "仓库",
              "width": 12
            },
            {
              "name": "类型",
              "width": 12
            },
            {
              "name": "计划",
              "width": 12
            },
            {
              "width": 12,
              "name": "单品"
            },
            {
              "name": "编码",
              "width": 12
            },
            {
              "name": "用户",
              "width": 12
            },
            {
              "width": 12,
              "name": "凭据"
            },
            {
              "name": "单品编码",
              "width": 12
            },
            {
              "name": "计划编码",
              "width": 12
            },
            {
              "name": "发生时刻",
              "width": 12
            },
            {
              "name": "批次",
              "width": 12
            },
            {
              "name": "备注",
              "width": 12
            },
            {
              "name": "状态",
              "width": 12
            },
            {
              "name": "唯一字段",
              "width": 12
            },
            {
              "name": "创建时间",
              "width": 12
            },
            {
              "name": "更新时间",
              "width": 12
            }
          ]
        }
      ]
    }
  },
  "fields": {
    "form": {
      "唯一字段": {
        "bind": "unique_id",
        "view": {
          "type": "",
          "props": {}
        },
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 唯一字段"
          }
        }
      },
      "更新时间": {
        "bind": "updated_at",
        "edit": {
          "type": "DatePicker",
          "props": {
            "placeholder": "请选择 更新时间",
            "showTime": {
              "format": "HH:mm:ss"
            }
          }
        }
      },
      "单品编码": {
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 单品编码"
          }
        },
        "bind": "sku_sn",
        "view": {
          "type": "",
          "props": {}
        }
      },
      "创建时间": {
        "bind": "created_at",
        "edit": {
          "type": "DatePicker",
          "props": {
            "showTime": {
              "format": "HH:mm:ss"
            },
            "placeholder": "请选择 创建时间"
          }
        }
      },
      "仓库": {
        "bind": "warehouse_id",
        "edit": {
          "type": "InputNumber",
          "props": {
            "placeholder": "请输入 仓库"
          }
        }
      },
      "用户": {
        "bind": "user_id",
        "edit": {
          "type": "InputNumber",
          "props": {
            "placeholder": "请输入 用户"
          }
        }
      },
      "批次": {
        "view": {
          "props": {},
          "type": ""
        },
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 批次"
          }
        },
        "bind": "batch"
      },
      "凭据": {
        "bind": "ticket_id",
        "edit": {
          "props": {
            "placeholder": "请输入 凭据"
          },
          "type": "InputNumber"
        }
      },
      "ID": {
        "edit": {
          "type": "InputNumber",
          "props": {
            "placeholder": "请输入 ID",
            "disabled": true
          }
        },
        "bind": "id"
      },
      "单品": {
        "bind": "sku_id",
        "edit": {
          "type": "InputNumber",
          "props": {
            "placeholder": "请输入 单品"
          }
        }
      },
      "备注": {
        "bind": "remark",
        "view": {
          "type": "",
          "props": {}
        },
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 备注"
          }
        }
      },
      "计划": {
        "bind": "plan_id",
        "edit": {
          "props": {
            "placeholder": "请输入 计划"
          },
          "type": "InputNumber"
        }
      },
      "类型": {
        "bind": "type",
        "edit": {
          "type": "Select",
          "props": {
            "placeholder": "请选择 类型",
            "options": [
              {
                "label": "入库",
                "value": "入库"
              },
              {
                "label": "出库",
                "value": "出库"
              },
              {
                "label": "进入",
                "value": "进入"
              },
              {
                "label": "离开",
                "value": "离开"
              }
            ]
          }
        }
      },
      "编码": {
        "bind": "sn",
        "view": {
          "type": "",
          "props": {}
        },
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 编码"
          }
        }
      },
      "发生时刻": {
        "bind": "uptime",
        "edit": {
          "type": "DatePicker",
          "props": {
            "showTime": {
              "format": "HH:mm:ss"
            },
            "placeholder": "请选择 发生时刻"
          }
        }
      },
      "状态": {
        "edit": {
          "type": "Select",
          "props": {
            "placeholder": "请选择 状态",
            "options": [
              {
                "label": "录入",
                "value": "录入"
              },
              {
                "value": "生效",
                "label": "生效"
              },
              {
                "label": "异常",
                "value": "异常"
              },
              {
                "label": "归档",
                "value": "归档"
              }
            ]
          }
        },
        "bind": "status"
      },
      "计划编码": {
        "bind": "plan_sn",
        "view": {
          "props": {},
          "type": ""
        },
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 计划编码"
          }
        }
      }
    }
  },
  "config": {
    "full": true
  }
}
