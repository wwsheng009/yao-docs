{
  "name": "导出任务下载",
  "action": {
    "bind": {
      "model": "exports",
      "option": {
        "withs": {}
      }
    }
  },
  "layout": {
    "primary": "id",
    "actions": [
      {
        "title": "返回",
        "icon": "icon-arrow-left",
        "showWhenAdd": true,
        "showWhenView": true,
        "action": [
          {
            "name": "CloseModal",
            "type": "Common.closeModal",
            "payload": {}
          }
        ]
      },
      {
        "title": "保存",
        "icon": "icon-check",
        "style": "primary",
        "showWhenAdd": true,
        "action": [
          {
            "name": "Submit",
            "type": "Form.submit",
            "payload": {}
          },
          {
            "name": "Back",
            "type": "Common.closeModal",
            "payload": {}
          }
        ]
      },
      {
        "icon": "icon-trash-2",
        "style": "danger",
        "title": "Delete",
        "action": [
          {
            "name": "Confirm",
            "type": "Common.confirm",
            "payload": {
              "title": "确认删除",
              "content": "删除后不可撤销！"
            }
          },
          {
            "name": "Delete",
            "payload": {
              "model": "exports"
            },
            "type": "Form.delete"
          },
          {
            "name": "Back",
            "type": "Common.closeModal",
            "payload": {}
          }
        ]
      }
    ],
    "form": {
      "sections": [
        {
          "columns": [
            {
              "name": "ID",
              "width": 12
            },
            {
              "name": "名称",
              "width": 12
            },
            {
              "name": "表格名称",
              "width": 12
            },
            {
              "name": "下载地址",
              "width": 12
            },
            {
              "width": 12,
              "name": "状态"
            },
            {
              "name": "创建时间",
              "width": 12
            },
            {
              "name": "更新时间",
              "width": 12
            }
          ]
        }
      ]
    }
  },
  "fields": {
    "form": {
      "名称": {
        "bind": "name",
        "view": {
          "type": "",
          "props": {}
        },
        "edit": {
          "props": {
            "placeholder": "请输入 名称"
          },
          "type": "Input"
        }
      },
      "表格名称": {
        "bind": "table_name",
        "view": {
          "type": "",
          "props": {}
        },
        "edit": {
          "type": "Input",
          "props": {
            "placeholder": "请输入 表格名称"
          }
        }
      },
      "下载地址": {
        "edit": {
          "props": {
            "placeholder": "请输入 下载地址"
          },
          "type": "Input"
        },
        "bind": "url",
        "view": {
          "type": "",
          "props": {}
        }
      },
      "状态": {
        "bind": "status",
        "edit": {
          "type": "Select",
          "props": {
            "options": [
              {
                "label": "",
                "value": ""
              },
              {
                "label": "未完成",
                "value": "未完成"
              },
              {
                "label": "已完成",
                "value": "已完成"
              },
              {
                "label": "导出错误",
                "value": "导出错误"
              }
            ],
            "placeholder": "请选择 状态"
          }
        }
      },
      "创建时间": {
        "bind": "created_at",
        "edit": {
          "type": "DatePicker",
          "props": {
            "placeholder": "请选择 创建时间",
            "showTime": {
              "format": "HH:mm:ss"
            }
          }
        }
      },
      "更新时间": {
        "edit": {
          "type": "DatePicker",
          "props": {
            "placeholder": "请选择 更新时间",
            "showTime": {
              "format": "HH:mm:ss"
            }
          }
        },
        "bind": "updated_at"
      },
      "ID": {
        "bind": "id",
        "edit": {
          "type": "InputNumber",
          "props": {
            "placeholder": "请输入 ID",
            "disabled": true
          }
        }
      }
    }
  },
  "config": {
    "full": true
  }
}
